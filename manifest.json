{
  "name": "CORS Mask",
  "version": "1.0",
  "description": "a chrome extension that helps avoiding CORS restrict",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/mask16.png",
      "32": "images/mask32.png",
      "48": "images/mask48.png",
      "128": "images/mask128.png"
    }
  },
  // "content_scripts": 
  //   [
  //     {
  //         "matches": ["<all_urls>"],
  //         "js": ["content/content.js"],
  //         "run_at": "document_start"
  //     }
  //   ],
  // "options_page": "options/options.html",
  "icons": {
    "16": "images/mask16.png",
    "32": "images/mask32.png",
    "48": "images/mask48.png",
    "128": "images/mask128.png"
  },
  "manifest_version": 2
}
